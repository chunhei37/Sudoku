<VBox minWidth="-Infinity" prefHeight="400.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SudokuController">
  <children>
    <MenuBar accessibleRole="LIST_VIEW" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#newGame" text="New" />
            <MenuItem mnemonicParsing="false" onAction="#open" text="Open…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" onAction="#save" text="Save" />
            <MenuItem mnemonicParsing="false" onAction="#saveAs" text="Save As…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" onAction="#exit" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="375.0" prefWidth="540.0" VBox.vgrow="ALWAYS">
      <children>
            <Line fx:id="line1" endY="360.0" fill="BLACK" layoutX="126.0" startY="10.0" strokeWidth="2.0" />
            <Line fx:id="line2" endX="360.0" fill="BLACK" layoutY="127.0" startX="10.0" strokeWidth="2.0" />
            <Line fx:id="line3" endX="360.0" fill="BLACK" layoutY="244.0" startX="10.0" strokeWidth="2.0" />
            <Line fx:id="line4" endY="349.0" fill="BLACK" layoutX="243.0" layoutY="10.0" strokeWidth="2.0" />
            <HBox prefHeight="375.0" prefWidth="373.0">
               <children>
                  <GridPane fx:id="mainGrid" gridLinesVisible="true" maxHeight="350.0" maxWidth="350.0" prefHeight="350.0" prefWidth="350.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <HBox.margin>
                        <Insets left="10.0" top="10.0" />
                     </HBox.margin>
                  </GridPane>
               </children>
            </HBox>
            <AnchorPane fx:id="win_msg" layoutY="2.0" prefHeight="200.0" prefWidth="800.0" visible="false">
               <children>
                  <Rectangle fx:id="winBg" arcHeight="5.0" arcWidth="5.0" fill="#9CBA7F" height="80.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="550.0" y="140.0" />
                  <Text fx:id="winText" fill="WHITE" layoutY="-134.0" strokeType="OUTSIDE" strokeWidth="0.0" text="You Solved The Puzzle!" textAlignment="CENTER" wrappingWidth="535.8718872070312" x="10.0" y="325.0">
                     <font>
                        <Font name="DINPro-Medium" size="33.0" />
                     </font>
                  </Text>
             	</children>
            </AnchorPane>
            <AnchorPane fx:id="error_msg" layoutY="2.0" prefHeight="200.0" prefWidth="800.0" visible="false">
               <children>
                  <Rectangle fx:id="errorBg" arcHeight="5.0" arcWidth="5.0" fill="#B33A3A" height="80.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="550.0" y="140.0" />
                  <Text fx:id="errorText" fill="WHITE" layoutY="-134.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wrong! Please Check!" textAlignment="CENTER" wrappingWidth="468.9439697265625" x="40.0" y="325.0">
                     <font>
                        <Font name="DINPro-Medium" size="36.0" />
                     </font>
                  </Text>
               </children>
            </AnchorPane>
            <ComboBox fx:id="cmbBoxNewGame" layoutX="400.0" layoutY="51.0" onAction="#newGameDifficulty" prefHeight="25.0" prefWidth="108.0" promptText="New Game">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:id="easy" fx:value="Easy" />
                            <String fx:id="medium" fx:value="Medium" />
                            <String fx:id="hard" fx:value="Hard" />
                        </FXCollections>
                    </items>
            </ComboBox>
            <Button fx:id="check_btn" layoutX="400.0" layoutY="87.0" mnemonicParsing="false" onAction="#checkPuzzle" prefHeight="25.0" prefWidth="108.0" text="Check Puzzle" />
            <Button fx:id="clear_btn" layoutX="400.0" layoutY="124.0" mnemonicParsing="false" onAction="#clear" prefHeight="25.0" prefWidth="108.0" text="Clear Puzzle" />
            <Button fx:id="solveAll_btn" layoutX="400.0" layoutY="160.0" mnemonicParsing="false" onAction="#solvePuzzle" prefHeight="25.0" prefWidth="108.0" text="Solve" />
      </children>
    </AnchorPane>
  </children>
</VBox>
